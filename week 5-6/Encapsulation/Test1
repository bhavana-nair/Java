package Second;

public class Test1{

	public static void main(String args[]){
		
		//WORKING IN PARENT OBJECT IN SAME PACKAGE
		
		AccessModifiers n = new AccessModifiers();
		
		System.out.println(n.a);
		//ERROR// System.out.println(n.b);
		System.out.println(n.c);
		
	
		n.publicAccessModifier();
		//protected modifier accessed within the package
		n.protectedAccessModifier();
		//ERROR//n.privateAccessModifier();  //The method privateAccessModifier() from the type AccessModifiers is not visible
	}
	
	
	
}
